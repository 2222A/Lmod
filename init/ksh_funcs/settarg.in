# -*- shell-script -*-
# This file defines a ksh alias.
# It sometimes ends up in a zsh directory # so that zsh users can run this shell function with ksh scripts
if [ -n "${KSH_VERSION+x}" ]; then
  if [ "${LMOD_SETTARG_CMD:-:}" != ":" ]; then
    settarg () {
      eval $(${LMOD_SETTARG_CMD:-:} -s sh "$@" )
    }
  fi
fi

