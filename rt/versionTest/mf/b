#%Module#####################################################################
if { [info exists ::env(LMOD_VERSION) ] } {
  puts stderr "I am here"
}

if { $env(ModuleTool) == "Lmod" } { 
  puts stderr "My module tool is Lmod"
}

if { [ versioncmp $env(ModuleToolVersion)  "8.4" ] > 0} { 
  puts stderr "My module version is newer than 8.4"
}



