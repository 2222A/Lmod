#%Module

proc ModulesHelp { } {
    puts stderr {   The GNU Compiler Collection includes front ends for C, C++, Objective-C, Fortran, Java, and Ada,
 as well as libraries for these languages (libstdc++, libgcj,...). - Homepage: http://gcc.gnu.org/
    }
}

module-whatis {Description: The GNU Compiler Collection includes front ends for C, C++, Objective-C, Fortran, Java, and Ada,
 as well as libraries for these languages (libstdc++, libgcj,...). - Homepage: http://gcc.gnu.org/}

set root    /private/tmp/software/Core/compiler/GCC/4.8.2

set mproot           "MODULEPATH_ROOT"
set modulepath_root  $env($mproot)

conflict    GCC
prepend-path	MODULEPATH		$modulepath_root/Compiler/GCC/4.8.2/base:$modulepath_root/Compiler/GCC/4.8.2/mpi:$modulepath_root/Compiler/GCC/4.8.2/system

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LD_LIBRARY_PATH		$root/lib/gcc/x86_64-apple-darwin13.2.0/4.8.2
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin

setenv	EBROOTGCC		"$root"
setenv	EBVERSIONGCC		"4.8.2"
setenv	EBDEVELGCC		"$root/easybuild/Core-compiler-GCC-4.8.2-easybuild-devel"


