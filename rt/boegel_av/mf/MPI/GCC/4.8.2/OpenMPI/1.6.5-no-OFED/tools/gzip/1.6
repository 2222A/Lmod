#%Module

proc ModulesHelp { } {
    puts stderr {   gzip (GNU zip) is a popular data compression program as a replacement for compress - Homepage: http://www.gnu.org/software/gzip/
    }
}

module-whatis {Description: gzip (GNU zip) is a popular data compression program as a replacement for compress - Homepage: http://www.gnu.org/software/gzip/}

set root    /tmp/software/MPI/GCC/4.8.2/OpenMPI/1.6.5-no-OFED/tools/gzip/1.6

